<div id="<%= dom_id team %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= team.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Members:</strong>
    <%= team.members.map(&:full_name).join(", ") %>
  </div>
</div>
